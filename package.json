{
  "name": "gatsby-plugin-theme-ui-preview-workspace",
  "workspaces": [
    "packages/*",
    "example"
  ],
  "private": true,
  "scripts": {
    "prettier": "prettier \"**/*.+(js|jsx|ts|tsx|json|yml|yaml|css|less|scss|md|mdx|graphql)\"",
    "format": "yarn prettier -- --write",
    "publish": "lerna publish"
  },
  "devDependencies": {
    "lerna": "^3.16.4",
    "prettier": "^1.18.2"
  }
}
